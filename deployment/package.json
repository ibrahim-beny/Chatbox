{
  "name": "chatbox-deployment",
  "version": "1.0.0",
  "description": "MVP-011: Blue/Green Deployment System for Chatbox Widget",
  "main": "blue-green-setup.js",
  "scripts": {
    "start": "node blue-green-setup.js",
    "start:blue": "node server-blue.js",
    "start:green": "node server-green.js",
    "deploy": "node deploy-script.js",
    "rollback": "curl -X POST http://localhost:8080/deployment/rollback",
    "status": "curl http://localhost:8080/deployment/status",
    "health": "curl http://localhost:8080/health",
    "switch": "curl -X POST http://localhost:8080/deployment/switch",
    "test:deployment": "node test-deployment.js",
    "test:rollback": "node test-rollback.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "http-proxy-middleware": "^2.0.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0"
  },
  "keywords": [
    "deployment",
    "blue-green",
    "rollback",
    "zero-downtime",
    "chatbox",
    "mvp-011"
  ],
  "author": "Chatbox Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
